{
  "name": "module-streamlabs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "prestart:prod": "tsc -p .",
    "start:prod": "node dist/index.js",
    "prebuild:x64": "tsc -p .",
    "prebuild:arm64": "tsc -p .",
    "build:x64": "npm-run-all build:x64:*",
    "build:arm64": "npm-run-all build:x64:*",
    "build:x64:linux": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/linux-x64/streamlabs/streamlabs --targets node12-linux-x64",
    "build:x64:macos": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/macos-x64/streamlabs/streamlabs --targets node12-macos-x64",
    "build:x64:win": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/win-x64/streamlabs/streamlabs --targets node12-win-x64",
    "build:arm64:linux": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/linux-arm64/streamlabs/streamlabs --targets node12-linux-arm64",
    "build:arm64:macos": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/macos-arm64/streamlabs/streamlabs --targets node12-macos-arm64",
    "build:arm64:win": "cross-env PKG_CACHE_PATH=./pkg-cache pkg . --output ./build/win-arm64/streamlabs/streamlabs --targets node12-win-arm64",
    "lint": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      "node_modules/evntcom-js/dist/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvntBoard/plugin-obs.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvntBoard/plugin-streamlabs/issues"
  },
  "homepage": "https://github.com/EvntBoard/plugin-streamlabs#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "evntcom-js": "https://github.com/EvntBoard/evntcom-js.git",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.3.2",
    "prettier": "^2.4.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.4.3"
  }
}
